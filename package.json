{
  "name": "better-logs",
  "displayName": "Better Logs",
  "description": "Improve developer workflow by making console logging fast and cleaning logs easy.",
  "version": "0.0.1",
  "publisher": "codex",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:betterLogs.insertConsoleLog",
    "onCommand:betterLogs.insertConsoleLogWithTemplate",
    "onCommand:betterLogs.trimConsoleLogs",
    "onCommand:betterLogs.toggleConsoleLogsMuted"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "betterLogs.insertConsoleLog",
        "title": "Better Logs: Insert console.log"
      },
      {
        "command": "betterLogs.insertConsoleLogWithTemplate",
        "title": "Better Logs: Insert console.log with template"
      },
      {
        "command": "betterLogs.trimConsoleLogs",
        "title": "Better Logs: Trim console.logs"
      },
      {
        "command": "betterLogs.toggleConsoleLogsMuted",
        "title": "Better Logs: Toggle console.log mute"
      }
    ],
    "keybindings": [
      {
        "command": "betterLogs.insertConsoleLog",
        "key": "ctrl+alt+l",
        "mac": "cmd+alt+l",
        "when": "editorTextFocus"
      },
      {
        "command": "betterLogs.insertConsoleLogWithTemplate",
        "key": "ctrl+alt+t",
        "mac": "cmd+alt+t",
        "when": "editorTextFocus"
      },
      {
        "command": "betterLogs.trimConsoleLogs",
        "key": "ctrl+alt+shift+l",
        "mac": "cmd+alt+shift+l",
        "when": "editorTextFocus"
      },
      {
        "command": "betterLogs.toggleConsoleLogsMuted",
        "key": "ctrl+alt+m",
        "mac": "cmd+alt+m",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -watch -p .",
    "test": "mocha -r ts-node/register -r ./src/test/vscode-mock.ts src/**/*.test.ts"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.0.0",
    "mocha": "^10.7.3",
    "ts-node": "^10.9.2",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "typescript": "^5.5.4"
  }
}
